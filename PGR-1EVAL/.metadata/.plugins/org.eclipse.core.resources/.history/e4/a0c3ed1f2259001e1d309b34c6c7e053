package main;

import java.util.Scanner;

public class Ej11 {

	public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		int i=1;
		int u;
		System.out.println("Introduce un numero para calcular los impares");
		
		u=sc.nextInt();

		while (1 < u) {
			
			if(i%2!=0) {
				System.out.println(i);
			}
			i++;
		}
		

	}

}
